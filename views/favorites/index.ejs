<div class="well">
    <h4>Your Favorite Cocktails</h4>
    <ul>
        <% fav.forEach(function(favorite) { %>
            <li><a href="/favorites/<%= favorite.idDrink %>"><%= favorite.name %></a></li>
            <form method="POST" action="/favorites/<%= favorite.idDrink %>/?_method=DELETE">
                <input type="submit" value="Delete <%=favorite.name %>">
            </form>
        <% }) %>
    </ul>
</div>