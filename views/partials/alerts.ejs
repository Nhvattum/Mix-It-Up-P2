<% if (alerts.error) { %>
    <% alerts.error.forEach(function(message) { %>
        <div class="alert alert-danger" role="alert">
            <%= message %>
        </div>
    <% }); %> 
        
<% } else if (alerts.success) { %>
    <% alerts.success.forEach(function(message) { %>
        <div class="alert alert-success" role="alert" >
            <%= message %>
        </div>
    <% }) %>
<% } %>
<script>
    console.log(alerts)
</script>