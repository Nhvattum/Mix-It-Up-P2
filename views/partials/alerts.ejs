<% if (alerts.error) { %>
    <% alerts.error.forEach(function(message) { %>
        <div style="background-color: red; color: white;">
            <%= message %>
        </div>
    <% }); %> 
        
<% } else if (alerts.success) { %>
    <% alerts.success.forEach(function(message) { %>
        <div style="background-color: green; color: white;">
            <%= message %>
        </div>
    <% }) %>
<% } %>
<script>
    console.log(alerts)
</script>